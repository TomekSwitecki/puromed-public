{
  "name": "react-scss-boilerplate",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@vectopus/atlas-icons-react": "^0.0.10",
    "boxicons": "^2.1.4",
    "date-fns": "^2.30.0",
    "i18next": "^23.5.1",
    "react": "^18.2.0",
    "react-calendar": "^4.6.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^13.3.0",
    "react-router-dom": "^6.16.0",
    "react-scripts": "^5.0.1",
    "react-svg": "^16.1.26",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
